#include <stdio.h>

int main() {
    int pages[20], frames[3], i, j, k, faults = 0;

    for (i = 0; i < 3; i++) frames[i] = -1;

    printf("Enter pages:\n");
    for (i = 0; i < 10; i++) scanf("%d", &pages[i]);

    for (i = 0; i < 10; i++) {
        int found = 0;
        for (j = 0; j < 3; j++)
            if (frames[j] == pages[i]) found = 1;

        if (!found) {
            frames[faults % 3] = pages[i];
            faults++;
        }
    }
    printf("Page Faults = %d\n", faults);
    return 0;
}
