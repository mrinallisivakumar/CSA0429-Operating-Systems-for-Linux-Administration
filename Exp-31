#include <stdio.h>

int main() {
    int pages[20], frames[3] = {-1,-1,-1}, i, j, k = 0, faults = 0;

    printf("Enter pages:\n");
    for (i = 0; i < 10; i++)
        scanf("%d", &pages[i]);

    for (i = 0; i < 10; i++) {
        int found = 0;
        for (j = 0; j < 3; j++)
            if (frames[j] == pages[i]) found = 1;

        if (!found) {
            frames[k] = pages[i];
            k = (k + 1) % 3;
            faults++;
        }
    }
    printf("Page Faults = %d\n", faults);
    return 0;
}
