#include <stdio.h>

int main() {
    FILE *fp;
    char data[100];

    fp = fopen("file.txt", "w");
    printf("Enter data to write: ");
    gets(data);
    fprintf(fp, "%s", data);
    fclose(fp);

    fp = fopen("file.txt", "r");
    printf("\nFile content:\n");
    fgets(data, 100, fp);
    printf("%s", data);
    fclose(fp);

    return 0;
}
